		"css-loader": "^6.5.1",
		"dotenv": "^16.0.1",
		"file-loader": "^6.2.0",
		"react": "^18.0.0",
		"react-bootstrap": "^2.4.0",
		"react-dom": "^18.0.0",
		"react-icons": "^4.3.1",
		"sass": "^1.49.0",
		"sass-loader": "^12.3.0",
		"style-loader": "^3.3.1",
		"styled-components": "^5.3.5",

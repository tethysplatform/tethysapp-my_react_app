		"jest-fetch-mock": "^3.0.3",
		"react-test-renderer": "^18.1.0"
	},
	"jest": {
		"testRegex": "(.+).test.js",
		"testEnvironment": "jsdom",
		"moduleFileExtensions": [
			"js"
		],
		"moduleDirectories": [
			"node_modules"
		],
		"moduleNameMapper": {
			"\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/reactapp/tests/mocks/fileMock.js",
			"\\.(css|less)$": "identity-obj-proxy"
		},
		"setupFilesAfterEnv": [
			"<rootDir>/reactapp/tests/setupTests.js"
		]
	}
}
